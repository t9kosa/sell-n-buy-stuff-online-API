{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example1.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "matti85",
            "firstName": "matti",
            "lastName": "meikalainen",
            "email": "matti.meika@gmail.com",
            "password": "matti",
            "dateOfBirth": "1985-05-25"
        }
    ],
    "required": [
        "username",
        "firstName",
        "lastName",
        "email",
        "password",
        "dateOfBirth"
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "matti85"
            ]
        },
        "firstName": {
            "$id": "#/properties/firstName",
            "type": "string",
            "title": "The firstName schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "matti"
            ]
        },
        "lastName": {
            "$id": "#/properties/lastName",
            "type": "string",
            "title": "The lastName schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "meikalainen"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "matti.meika@gmail.com"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "matti"
            ]
        },
        "dateOfBirth": {
            "$id": "#/properties/dateOfBirth",
            "type": "string",
            "title": "The dateOfBirth schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "1985-05-25"
            ]
        }
    },
    "additionalProperties": true
}